#:kivy 2.0.0
#:import utils kivy.utils 
<Button>
    font_size: 32
    size_hint: (0.3, 0.3)




<ClientUI>

    canvas:
        Color:
            rgb:utils.get_color_from_hex('#333333') 
        Rectangle:
            pos: self.pos
            size: self.size

    canvas:
        Color:
            rgb:utils.get_color_from_hex('#c49102') 
        Rectangle:
            pos: self.pos
            size: 500, 500




    FloatLayout:
        size: root.width, root.height
        orientation: 'vertical'

        TextInput:
            id: myInput
            size_hint: (0.3,0.3)
            font_size: 50
            pos_hint: {'top': 1}
        Button:
            text: 'SEND'
            # {"x", "y", "top", "bottom", "left", "right"}
            pos_hint: {'bottom': 0}
        Button:
            text: 'SEND'
            # {"x", "y", "top", "bottom", "left", "right"}
            pos_hint: {'bottom': 0, "x": 0.3}
            on_press: root.testpress(10)